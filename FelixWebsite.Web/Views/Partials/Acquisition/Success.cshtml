@using FelixResources = FelixWebsite.Core.App_GlobalResources.FelixResources;

@{

}

<section id="transfer" class="margin-top-40">
    <div class="container">
        <div class="row">
            <!-- Buitenkant -->
            <div id="panel-outside" class="panel panel-default">
                <div class="panel-body checkboxes">
                    <div class="col-sm-12 text-center margin-bottom-20 margin-top-20">
                        <div class="section-title">
                            <h1 class="center">@FelixResources.general_complete</h1>
                        </div>
                        <div class="row margin-top-60">
                            <h1 class="margin-bottom-30"><i class="fas fa-check-circle"></i></h1>
                            <h2 class="margin-bottom-30">@FelixResources.general_success</h2>
                            <div class="col-md-12">
                                <p class="text-center success-takeover">
                                    @FelixResources._2dehands_success_successMessage<br>
                                    @FelixResources._2dehands_success_successMessageContinue <a id="successUserEmail"></a>
                                </p>
                                <p class="text-center success-damages">
                                    @FelixResources._2dehands_success_successMessage_damage<br>
                                    @FelixResources._2dehands_success_successMessageContinue_damage <a id="successUserEmail"></a>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>