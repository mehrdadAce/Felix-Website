@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<Umbraco.Web.PublishedContentModels.Errorpage>
@using FelixWebsite.Core.App_GlobalResources
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Error 500</title>
    <link rel="icon" type="image/png" href="../../favicon.png" sizes="16x16">
    <link rel="stylesheet" type="text/css" href="../../bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="../../css/shared/shared.css">
    <link rel="stylesheet" type="text/css" href="../../css/felix-errors.css">
</head>
<body class="server-error">
    <div class="container v-center">
        <div class="col-sm-6 col-sm-offset-3">
            <div class="panel panel-default">
                <div class="panel-body">
                    <h1 class="margin-bottom-30 margin-top-30 text-center">@FelixResources.general_error_header</h1>
                    <h4 class="margin-bottom-30 margin-top-30 text-center">@FelixResources.general_error_body</h4>
                    <div class="row">
                        <div class="col-sm-6">
                            <a class="link-button" href="/">@FelixResources.general_HomePage</a>
                        </div>
                        <div class="col-sm-6">
                            <a class="link-button" href="#" onclick="GoToPreviousPage();return false;">@FelixResources.general_goToPreviousPage</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>

</html>
<script type="text/javascript">
    function GoToPreviousPage() {
        window.history.back();
    }
</script>