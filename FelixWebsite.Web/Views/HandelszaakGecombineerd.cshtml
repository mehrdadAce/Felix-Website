@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.BusinessCombined>
@using FelixWebsite.Bll.Helpers
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
    Layout = "Shared/Master.cshtml";
}
<link rel="stylesheet" type="text/css" href="~/css/felix-business.css">
@Html.Partial("Slider/OptionalSlider", Model.Content)
@Html.Partial("Actions/Actions", Model)
@Html.Partial("Stores/Stores", Model)
@Html.Partial("News/News", Model)
@Html.Partial("Reviews/Reviews", Model)
@Scripts.Render("~/Js/GoogleMaps")
<script async defer type='text/javascript' src='https://maps.googleapis.com/maps/api/js?key=@ConfigHelper.GoogleMapsApiKey&callback=initGoogleMap'></script>
